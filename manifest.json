{
    "manifest_version": 2,
    "name": "the-rock",
    "version": "1.0",
    "content_scripts": [
        {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "img/logo.png",
        "default_title": "The Rock"
    }
}